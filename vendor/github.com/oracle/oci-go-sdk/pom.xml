            <id>spec-conditionals-prefer-database</id>
            <phase>initialize</phase>
            <goals>
              <goal>prefer</goal>
            </goals>
            <configuration>
              <inputFiles>
                <!-- New layout: source/<spec.proto.yaml> -->
                <inputFile>${spec.temp.dir}/database/source/${database.spec.name}</inputFile>
                <!-- Old layout: ./<spec.proto.yaml> -->
                <inputFile>${spec.temp.dir}/database/${database.spec.name}</inputFile>
              </inputFiles>
              <outputFile>${preferred.temp.dir}/${database.spec.name}</outputFile>
            </configuration>
          </execution>
          <execution>
            <id>spec-conditionals-preprocess-database</id>
            <phase>initialize</phase>
